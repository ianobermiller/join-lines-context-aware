{
  "name": "join-lines-context-aware",
  "displayName": "Join Lines Context Aware",
  "description": "Join lines command that does what you expect with comments and string literals.",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/ianobermiller/join-lines-context-aware"
  },
  "publisher": "ianobermiller",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:join-lines-context-aware.join"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "join-lines-context-aware.join",
        "title": "Join Lines (Context Aware)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "vsce": "^1.75.0"
  }
}
